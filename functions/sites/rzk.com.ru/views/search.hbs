<div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-4 row-cols-xl-5 row-cols-xxl-6 g-2 mb-3">
  {{#each resalt.hits as |product|}}
    <div class="col">
      <div class="card text-center h-100">
        <div class="card-body">
          <h6>{{product.name}}</h6>
        </div>
      </div>
    </div>
  {{/each}}
</div>
<div class="row">
  <div class="col">
    <h5>Page: <strong>{{resalt.page}}</strong></h5>
    <h5>nbHits: <strong>{{resalt.nbHits}}</strong></h5>
    <h5>nbPages: <strong>{{resalt.nbPages}}</strong></h5>
    <h5>hitsPerPage: <strong>{{resalt.hitsPerPage}}</strong></h5>
  </div>
</div>
