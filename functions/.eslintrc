{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "max-len": ["error", {"code": 250}],
    "quotes": ["error", "double"],
    "new-cap": ["error", { "properties": false }],
    "require-jsdoc": 0
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
