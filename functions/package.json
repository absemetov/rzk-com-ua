{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "parcel-s": "parcel --target web",
    "parcel-b": "rm -rf sites/rzk.com.ru/dist && parcel build --target web",
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "targets": {
    "main": false,
    "web": {
      "context": "browser",
      "source": "sites/rzk.com.ru/src/index.html",
      "distDir": "sites/rzk.com.ru/dist"
    },
    "bundler": {
      "context": "node",
      "source": "sites/rzk.com.ru/src/app.js",
      "distDir": "sites/rzk.com.ru/dist",
      "sourceMap": false
    }
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.6.2",
    "@algolia/autocomplete-plugin-recent-searches": "^1.6.3",
    "@algolia/autocomplete-theme-classic": "^1.6.2",
    "@googleapis/content": "^1.2.0",
    "@popperjs/core": "^2.11.5",
    "algoliasearch": "^4.13.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "bootstrap": "5.2.0-beta1",
    "bootstrap-icons": "^1.8.2",
    "busboy": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "currency-codes": "^2.1.0",
    "cyrillic-to-translit-js": "^3.1.0",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.2",
    "firebase": "^8.4.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.21.2",
    "google-spreadsheet": "^3.2.0",
    "googleapis": "^76.0.0",
    "instantsearch.js": "^4.40.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "telegraf": "^4.7.0",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.5.0",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0",
    "parcel": "^2.5.0"
  },
  "private": true
}
