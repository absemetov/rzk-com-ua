{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "start-parcel-ru": "parcel sites/rzk.com.ru/src/index.html --dist-dir sites/rzk.com.ru/dist",
    "build-parcel-ru": "parcel build sites/rzk.com.ru/src/app.js --target server --dist-dir ../sites/rzk.com.ru/js/dist",
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "targets": {
    "main": false,
    "modern": {
      "engines": {
        "browsers": "Chrome 80"
      }
    },
    "server": {
      "context": "node",
      "outputFormat": "esmodule"
    }
  },
  "dependencies": {
    "@googleapis/content": "^1.2.0",
    "algoliasearch": "^4.13.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "busboy": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "currency-codes": "^2.1.0",
    "cyrillic-to-translit-js": "^3.1.0",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.2",
    "firebase": "^8.4.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.20.1",
    "google-spreadsheet": "^3.2.0",
    "googleapis": "^76.0.0",
    "instantsearch.js": "^4.40.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "telegraf": "^4.7.0",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0",
    "parcel": "^2.5.0"
  },
  "private": true
}
